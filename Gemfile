source "https://rubygems.org"

gem "github-pages", group: :jekyll_plugins

# תוספים נוספים שאתה עשוי להשתמש בהם
gem "jekyll-feed"
gem "jekyll-sitemap"
gem "jekyll-seo-tag"

# אם יש צורך ב-nokogiri להחלפת הטקסטים
gem "nokogiri"

# אם אתה משתמש במינימל מיסטייקס
gem "minimal-mistakes-jekyll", "~> 4.0"
